{
	"name": "kokopu-react",
	"version": "1.7.0",
	"description": "A React-based library to create and display chessboard and chess-related components.",
	"keywords": [
		"chess",
		"react",
		"chessboard",
		"ui",
		"games"
	],
	"bugs": "https://github.com/yo35/kokopu-react/issues",
	"repository": "github:yo35/kokopu-react",
	"license": "LGPL-3.0-or-later",
	"author": "Yoann Le Montagner <yo35@melix.net>",
	"funding": "https://www.paypal.com/donate?hosted_button_id=4EGWA4YEZSALW",
	"main": "dist/lib/index.js",
	"files": [
		"dist/lib",
		"doc_src",
		"graphic_test_src",
		"graphics",
		"scripts/clean-graphic-output.js",
		"scripts/docker-compose.yml",
		"scripts/test-graphic.webpack.config.js",
		"scripts/test-headless.webpack.config.js",
		"src",
		"test",
		".babelrc",
		".eslint.yml",
		".nycrc.yml"
	],
	"peerDependencies": {
		"kokopu": "^2.6.0",
		"react": "17.x.x || 18.x.x"
	},
	"dependencies": {
		"htmlparser2": "^8.0.1",
		"prop-types": "^15.8.1",
		"react-draggable": "^4.4.5",
		"react-motion": "^0.5.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.5",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@emotion/react": "^11.9.3",
		"@emotion/styled": "^11.9.3",
		"@mui/material": "^5.8.5",
		"babel-loader": "^8.2.5",
		"babel-plugin-istanbul": "^6.1.1",
		"copy-webpack-plugin": "^11.0.0",
		"coveralls": "^3.1.1",
		"css-loader": "^6.7.1",
		"eslint": "^8.18.0",
		"eslint-plugin-react": "^7.30.0",
		"html-webpack-plugin": "^5.5.0",
		"img-diff-js": "^0.5.2",
		"kokopu": "^2.9.0",
		"mocha": "^10.0.0",
		"null-loader": "^4.0.1",
		"nyc": "^15.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-styleguidist": "^11.2.0",
		"selenium-webdriver": "^4.1.2",
		"ssh2-sftp-client": "^9.0.1",
		"style-loader": "^3.3.1",
		"unit.js": "^2.1.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0"
	},
	"scripts": {
		"lint": "eslint doc_src graphic_test_src src test",
		"preunit": "webpack --config scripts/test-headless.webpack.config.js && webpack --config scripts/test-graphic.webpack.config.js && node scripts/clean-graphic-output.js",
		"unit": "nyc mocha --bail --sort",
		"test": "npm run lint && npm run unit",
		"publish_coverage": "nyc report --reporter=text-lcov | coveralls",
		"test_env:start": "docker-compose -p kokopu-react-test-env -f scripts/docker-compose.yml up -d",
		"test_env:stop": "docker-compose -p kokopu-react-test-env -f scripts/docker-compose.yml down",
		"build": "make -f scripts/build.mk",
		"clean": "make -f scripts/build.mk clean",
		"start": "styleguidist server --open --config scripts/styleguide.config.js",
		"predeploy": "npm run test && npm run build",
		"deploy": "npm publish && node scripts/deploy.js"
	}
}
