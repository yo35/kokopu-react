{
	"name": "kokopu-react",
	"version": "0.99.10",
	"description": "A React-based library to create and display chessboard widgets.",
	"keywords": [
		"chess",
		"react",
		"chessboard",
		"ui",
		"games"
	],
	"bugs": "https://github.com/yo35/kokopu-react/issues",
	"repository": "github:yo35/kokopu-react",
	"license": "LGPL-3.0-or-later",
	"author": "Yoann Le Montagner <yo35@melix.net>",
	"main": "dist/lib/index.js",
	"files": [
		"dist/lib"
	],
	"dependencies": {
		"kokopu": "^1.9.1",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-draggable": "^4.4.3",
		"react-motion": "^0.5.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.4",
		"@babel/preset-react": "^7.13.13",
		"@material-ui/core": "^4.11.4",
		"@material-ui/lab": "^4.0.0-alpha.58",
		"babel-loader": "^8.2.2",
		"css-loader": "^5.2.6",
		"eslint": "^7.27.0",
		"eslint-plugin-react": "^7.24.0",
		"html-webpack-plugin": "^5.3.1",
		"mocha": "^8.4.0",
		"null-loader": "^4.0.1",
		"style-loader": "^2.0.0",
		"unit.js": "^2.1.1",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2"
	},
	"scripts": {
		"lint": "eslint demo src test",
		"preunit": "webpack --config scripts/test_headless.webpack-config.js",
		"unit": "mocha --bail --sort",
		"test": "npm run lint && npm run unit",
		"build": "babel --delete-dir-on-start --copy-files -d dist/lib src",
		"start": "webpack serve --open --config scripts/demo.webpack-config.js",
		"predeploy": "npm run test && npm run build",
		"deploy": "npm publish"
	}
}
