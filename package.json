{
	"name": "kokopu-react",
	"version": "1.3.1",
	"description": "A React-based library to create and display chessboard components.",
	"keywords": [
		"chess",
		"react",
		"chessboard",
		"ui",
		"games"
	],
	"bugs": "https://github.com/yo35/kokopu-react/issues",
	"repository": "github:yo35/kokopu-react",
	"license": "LGPL-3.0-or-later",
	"author": "Yoann Le Montagner <yo35@melix.net>",
	"funding": "https://www.paypal.com/donate?hosted_button_id=4EGWA4YEZSALW",
	"main": "dist/lib/index.js",
	"files": [
		"dist/lib",
		"doc_src",
		"graphic_test_app",
		"graphics",
		"scripts/test_graphic",
		"scripts/test_headless.webpack-config.js",
		"src",
		"test",
		".babelrc",
		".eslint.yml",
		".nycrc.yml"
	],
	"dependencies": {
		"kokopu": "^2.2.0",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-draggable": "^4.4.4",
		"react-motion": "^0.5.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-react": "^7.14.5",
		"@emotion/react": "^11.5.0",
		"@emotion/styled": "^11.3.0",
		"@mui/material": "^5.0.4",
		"babel-loader": "^8.2.3",
		"babel-plugin-istanbul": "^6.1.1",
		"copy-webpack-plugin": "^9.0.1",
		"coveralls": "^3.1.1",
		"css-loader": "^6.4.0",
		"eslint": "^8.0.1",
		"eslint-plugin-react": "^7.26.1",
		"html-webpack-plugin": "^5.4.0",
		"img-diff-js": "^0.5.2",
		"mocha": "^9.1.3",
		"null-loader": "^4.0.1",
		"nyc": "^15.1.0",
		"prompt": "^1.2.0",
		"react-dom": "^17.0.2",
		"react-styleguidist": "^11.1.7",
		"selenium-webdriver": "^4.0.0",
		"ssh2-sftp-client": "^7.1.0",
		"style-loader": "^3.3.0",
		"unit.js": "^2.1.1",
		"webpack": "^5.59.0",
		"webpack-cli": "^4.9.1"
	},
	"scripts": {
		"lint": "eslint doc_src graphic_test_app src test",
		"preunit": "webpack --config scripts/test_headless.webpack-config.js && webpack --config scripts/test_graphic/webpack-config.js && node scripts/test_graphic/clean-graphic-output.js",
		"unit": "nyc mocha --bail --sort",
		"test": "npm run lint && npm run unit",
		"publish_coverage": "nyc report --reporter=text-lcov | coveralls",
		"start_test_env": "docker-compose -f scripts/test_graphic/docker-compose.yml up -d",
		"stop_test_env": "docker-compose -f scripts/test_graphic/docker-compose.yml down",
		"build": "make -f scripts/build.mk",
		"clean": "make -f scripts/build.mk clean",
		"start": "styleguidist server --open --config scripts/styleguide.config.js",
		"predeploy": "npm run test && npm run build",
		"deploy": "npm publish && node scripts/deploy.js"
	}
}
